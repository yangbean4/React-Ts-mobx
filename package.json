{
  "name": "ts-react-webpack",
  "version": "1.0.0",
  "description": "an admin starter-template with typescript, react, mobx and webpack...",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "add": "add-component",
    "lint": "tslint --fix --type-check --project ./tsconfig.json",
    "bean": "rimraf .cache-loader && yarn dev",
    "dev": "cross-env NODE_ENV=development APP_ENV=dev webpack-dev-server --config build/webpack.config.js --open --hot",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "build:qa": "cross-env APP_ENV=qa yarn build",
    "build:prod": "cross-env APP_ENV=prod yarn build"
  },
  "keywords": [
    "webpack",
    "typescript",
    "admin",
    "react",
    "mobx",
    "starter-template"
  ],
  "author": "jackple",
  "license": "MIT",
  "dependencies": {
    "antd": "3.16.1",
    "axios": "^0.18.0",
    "bourbon": "^5.1.0",
    "echarts": "^4.4.0",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mobx-react-router": "^4.0.5",
    "path-to-regexp": "^3.0.0",
    "react": "16.8.2",
    "react-color": "^2.17.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "16.8.2",
    "react-hot-loader": "^4.7.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@svgr/webpack": "^4.1.0",
    "@types/enzyme": "^3.1.18",
    "@types/enzyme-adapter-react-16": "^1.0.4",
    "@types/jest": "^24.0.6",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/react-loadable": "^5.5.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack-env": "^1.13.7",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "cache-loader": "^2.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "cross-env": "^5.1.4",
    "css-loader": "^2.1.0",
    "css-modules-typescript-loader": "^1.1.1",
    "customaddcomponents": "^1.0.35",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "raf": "^3.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-ydj": "^0.1.1",
    "stylelint-order": "^2.0.0",
    "terser-webpack-plugin": "^1.2.2",
    "thread-loader": "^2.1.2",
    "ts-jest": "^24.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslib": "^1.9.0",
    "tslint": "^5.12.1",
    "tslint-react": "^3.5.1",
    "typescript": "^3.3.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-hot-middleware": "^2.24.3",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|svg|jpg|jpeg|png|gif)$": "identity-obj-proxy",
      "^@constants/(.*)$": "<rootDir>/src/constants//$1",
      "^@services/(.*)$": "<rootDir>/src/services//$1",
      "^@utils/(.*)$": "<rootDir>/src/utils//$1",
      "^@assets/(.*)$": "<rootDir>/src/assets//$1",
      "^@components/(.*)$": "<rootDir>/src/components//$1",
      "^@views/(.*)$": "<rootDir>/src/containers/views//$1",
      "^@shared/(.*)$": "<rootDir>/src/containers/shared//$1",
      "^@config/(.*)$": "<rootDir>/src/config//$1"
    }
  }
}
